{
  "name": "pi-gen",
  "description": "Pingendo static site generator",
  "version": "1.0.17",
  "author": "Giulio Seregni @gseregni",
  "bin": {
    "pi-gen": "./bin/run"
  },
  "bugs": "https://github.com/gseregni/pi-gen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3",
    "@types/cheerio": "^0.22.10",
    "browser-sync": "^2.26.3",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.2",
    "get-caller-file": "^2.0.5",
    "install": "^0.12.2",
    "js-template": "^0.1.4",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "npm": "^6.8.0",
    "oclif": "^1.12.10",
    "pi-template": "^2.4.1",
    "shelljs": "^0.8.3",
    "tslib": "^1",
    "walk-sync": "^1.1.3"
  },
  "devDependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5",
    "@types/node": "^11.9.6",
    "ava": "^0.16.0",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "ghooks": "^1.2.4",
    "globby": "^8",
    "nyc": "^10.0.0",
    "standard": "^8.4.0",
    "ts-node": "^8.0.3",
    "tslint": "^5",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gseregni/pi-gen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pi-gen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "gseregni/pi-gen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/*.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "standard": "standard index.js"
  },
  "types": "lib/index.d.ts",
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
  }
}
